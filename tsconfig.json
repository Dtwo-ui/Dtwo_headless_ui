{
  "compilerOptions": {
    "target": "ES5",
//    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "types": ["node"],
  },
  "exclude": ["node_modules"],
  "include": [".eslintrc.js", "**/*.ts", "**/*.tsx",
    "rollup.config.js"
  ]
}
